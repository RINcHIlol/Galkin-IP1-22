<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="AvaloniaApplication1.AdminWindow"
        Title="AdminWindow">
  <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="10" Spacing="10">
    <TextBlock Text="You have successfully logged in."/>
      <Button Content="Add" Click="AddButton_Click" Width="200"/>
      <Button Content="Edit" Click="EditButton_Click" Width="200"/>
      <Button Content="Logout" Click="LogoutButton_Click" Width="200"/>
  </StackPanel>
</Window>
